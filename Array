1) function getLength(arr){
  return arr.length
}
2) function getFirst(arr){
    return arr[0]
}
3) function getLast(arr){
  return arr[arr.length-1]
}
4) function pushElement(arr){
  var el=1;
  arr.push(el)
  return arr
}
5) function popElement(arr){
  arr.pop()
  return arr
}

